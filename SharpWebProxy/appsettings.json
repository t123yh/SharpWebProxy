{
  "Kestrel": {
    "Endpoints": {
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  },
  "SiteConfig": {
    "ReplaceList": [
      "www.gstatic.com",
      "csdnimg.cn",
      "hm.baidu.com",
      "beacon.tingyun.com"
    ],
    "NoReplaceList": [
      "w3.org",
      "schema.org"
    ],
    "DomainNameReplacement": {
      "youtube": "ytb",
      "wikipedia": "wiki",
      "facebook": "fb"
    },
    "BlacklistedDomains": ["googlevideo", "dongtaiwang"],
    "MimeWhitelist": ["text/css", "application/javascript", "text/html", "text/javascript", "application/x-javascript"],
    "MimeWhitelistWithoutExtension": [
      "text/plain",
      "application/json"
    ]
  }
}